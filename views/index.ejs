<%- include('header') %>

<main>
  <div class="grid-row">
    <!DOCTYPE html>
    <html lang="en">
      <head>
        <title>Toys</title>
        <link rel="stylesheet" href="mystyles.css" />
      </head>
      <body>
        <%- include('header') %>
        <main>
          <div class="grid-row">
            <% mydata.forEach((toy)=> { %>
            <div class="grid-item">
              <%= toy.title.slice(0, 30) %>
              <div class="button">
                <a href="<%= toy.url %>" target="_blank"> BUY </a>
              </div>
            </div>
            <% }) %>
          </div>
        </main>
        <%- include('footer') %>
      </body>
    </html>
  </div>
</main>
